<div id="addFeedModel" class="reveal-modal" data-reveal>
  <% rss_feed = RssFeed.new() %>
  <%= simple_form_for(rss_feed, remote: true) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%#= f.input :title %>
      <%= f.input :feed_url %>
    </div>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
  <% end %>
  <a class="close-reveal-modal">&#215;</a>
</div>
